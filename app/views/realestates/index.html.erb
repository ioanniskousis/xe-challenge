<div class="form-outter">

  <h1>Properties Index</h1>

  <div class="index-container">
    <% if @realestates %>
      <% @realestates.each do |realestate| %>
        <%= link_to edit_realestate_path(realestate), :action => 'go' do %>
          <%= index_item(realestate) %>
        <% end %>
      <% end %>
    <% end %>
  </div>

</div>
